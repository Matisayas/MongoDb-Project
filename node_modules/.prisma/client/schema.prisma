generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id          String     @id @default(uuid())
  title       String
  subtitle    String
  description String
  status      StatusEnum
}

enum StatusEnum {
  TO_DO
  IN_PROGRESS
  DONE
}
